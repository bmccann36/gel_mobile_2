{
  "expo": {
    "name": "camerja",
    "description": "An example of using the Expo.Camera API. http://github.com/expo/camerja",
    "slug": "camerja",
    "privacy": "public",
    "sdkVersion": "UNVERSIONED",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
